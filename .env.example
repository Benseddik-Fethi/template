# =============================================================================
# SPRING BOOT TEMPLATE - Variables d'environnement
# =============================================================================
# Copier ce fichier en ".env" et remplir avec vos vraies valeurs

# -----------------------------------------------------------------------------
# DATABASE
# -----------------------------------------------------------------------------
DATABASE_URL=jdbc:postgresql://localhost:5432/myapp
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your-secure-password

# -----------------------------------------------------------------------------
# KEYCLOAK
# -----------------------------------------------------------------------------
KEYCLOAK_URL=http://localhost:8081
KEYCLOAK_REALM=my-realm
KEYCLOAK_CLIENT_ID=my-client
KEYCLOAK_CLIENT_SECRET=your-client-secret

KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# -----------------------------------------------------------------------------
# RUSTFS / S3
# -----------------------------------------------------------------------------
RUSTFS_ENDPOINT=https://s3.your-domain.com
RUSTFS_ACCESS_KEY=your-access-key
RUSTFS_SECRET_KEY=your-secret-key
RUSTFS_BUCKET_NAME=my-bucket
RUSTFS_REGION=us-east-1

# -----------------------------------------------------------------------------
# CORS
# -----------------------------------------------------------------------------
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4200

# -----------------------------------------------------------------------------
# SPRING PROFILES
# -----------------------------------------------------------------------------
SPRING_PROFILES_ACTIVE=dev
